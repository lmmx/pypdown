site_name: pypdown
theme:
  name: material
  palette:
    primary: blue
    accent: light blue
repo_name: lmmx/pypdown
repo_url: https://github.com/lmmx/pypdown
nav:
  - Home: index.md
  - Usage: usage.md
  - API Reference: api_reference.md
markdown_extensions:
  - pymdownx.highlight:
      anchor_linenums: true
  - pymdownx.inlinehilite
  - pymdownx.snippets
  - pymdownx.superfences